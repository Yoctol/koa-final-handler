{
  "name": "@yoctol/koa-final-handler",
  "description": "Final handler middleware for koa.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yoctol/koa-final-handler"
  },
  "version": "0.1.1",
  "main": "index.js",
  "files": ["index.js"],
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "test": "npm run lint && npm run testonly",
    "testonly": "jest"
  },
  "dependencies": {
    "http-status": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-yoctol-base": "^0.15.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "http-errors": "^1.6.3",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "koa-compose": "^4.0.0",
    "koa-context-validator": "^0.4.1",
    "lint-staged": "^7.1.0",
    "prettier": "^1.12.1",
    "prettier-package-json": "^1.6.0"
  },
  "keywords": ["final-handler", "handler", "koa", "middleware"],
  "engines": {
    "node": ">=7.6"
  },
  "jest": {
    "setupFiles": ["./test/configure-env.js"],
    "testEnvironment": "node",
    "testPathIgnorePatterns": ["node_modules/"]
  },
  "lint-staged": {
    "*.{json,md}": ["prettier --write", "git add"],
    "*.js": ["eslint --fix", "git add"],
    "package.json": [
      "prettier-package-json --write",
      "prettier --write",
      "git add"
    ],
    ".babelrc": ["prettier --parser json --write", "git add"]
  }
}
